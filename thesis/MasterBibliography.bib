@article{Not10,
	Annote = {Satellite derived global horizontal solar irradiance (GHI) from the SUNY modeled dataset in the National Solar Radiation Database (NSRDB) was compared to measurements from 27 weather stations in California during the years 1998--2005. The statistics of spatial and temporal differences between the two datasets were analyzed and related to meteorological phenomena. Overall mean bias errors (MBE) of the NSRDB--SUNY indicated a GHI overprediction of 5{\%}, which is smaller than the sensor accuracy of ground stations. However, at coastal sites, year-round systematic positive MBEs in the NSRDB--SUNY data up to 18{\%} were observed and monthly MBEs increased up to 54{\%} in the summer months during the morning. These differences were explained by a tendency for the NSRDB--SUNY model to overestimate GHI under cloudy conditions at the coast during summer mornings. A persistent positive evening MBE which was independent of site location and cloudiness occurred at all stations and was explained by an error in the time-shifting method applied in the NSRDB--SUNY. A correction method was derived for these two errors to improve the accuracy of the NSRDB--SUNY data in California.},
	Author = {Nottrott, Anders and Kleissl, Jan},
