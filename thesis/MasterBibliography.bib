	Journal = {Science},
	Number = {6048},
	Pages = {1393-1400},
	Title = {Visualizing Uncertainty About the Future},
	Url = {http://www.sciencemag.org/content/333/6048/1393.abstract},
	Volume = {333},
	Year = {2011},
	Bdsk-Url-1 = {http://www.sciencemag.org/content/333/6048/1393.abstract},
