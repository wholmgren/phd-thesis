	Doi = {http://dx.doi.org/10.1016/j.solener.2014.01.024},
	Isbn = {0038-092X},
	Journal = {Solar Energy},
	Keywords = {Threshold distance; Space--time forecast; Parameter shrinkage; L--method},
	Month = {5},
	Number = {0},
	Pages = {550--562},
	Title = {Solar irradiance forecasting using spatio-temporal empirical kriging and vector autoregressive models with parameter shrinkage},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S0038092X14000425},
	Volume = {103},
	Year = {2014},
