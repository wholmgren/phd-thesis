	Journal = {Solar Energy},
	Keywords = {Direct normal irradiance; DNI; Irradiance forecast; Model output statistics; Plant simulation; CSP-FoSyS},
	Month = {7},
	Number = {0},
	Pages = {244--255},
	Title = {Economic merits of a state-of-the-art concentrating solar power forecasting system for participation in the Spanish electricity market},
	Ty = {JOUR},
