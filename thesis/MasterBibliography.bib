	Keywords = {SUNY model; DNI validation; Variability index; DNI variability; Solar irradiance variability; Short-term variability},
	Month = {1},
	Number = {0},
	Pages = {246--258},
	Title = {Verification of the SUNY direct normal irradiance model with ground measurements},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S0038092X13004908},
	Volume = {99},
