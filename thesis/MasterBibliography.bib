	Keywords = {Stationarity; Anisotropy; Separability; Full symmetry; Variance-covariance structures; Time-forward kriging},
	Month = {12},
	Number = {0},
	Pages = {235--245},
	Title = {Solar irradiance forecasting using spatial-temporal covariance structures and time-forward kriging},
	Ty = {JOUR},
